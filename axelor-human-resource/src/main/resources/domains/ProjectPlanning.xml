<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="project" package="com.axelor.apps.project.db"/>

  <entity name="ProjectPlanning">

    <string name="name" required="true"/>
    <many-to-many name="employeeSet" ref="com.axelor.apps.hr.db.Employee"
      title="Employees" required="true"/>
    <many-to-many name="projectSet" ref="com.axelor.apps.project.db.Project"
      title="Projects" required="true"/>

  </entity>

</domain-models>